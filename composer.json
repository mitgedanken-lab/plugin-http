{
    "name": "phergie/phergie-irc-plugin-http",
    "description": "Phergie plugin for Provide HTTP functionality to other plugins",
    "keywords": [ "irc", "bot", "react", "plugin", "phergie", "phergieplugin" ],
    "license": "MIT",
    "authors": [
      {
        "name": "Cees-Jan Kiewiet",
        "email": "ceesjank@gmail.com"
      }
    ],
    "support": {
        "irc": "irc://irc.freenode.net/phergie"
    },
    "require": {
        "php": "^7.0||^5.6",
        "phergie/phergie-irc-bot-react": "~2.0",
        "wyrihaximus/react-guzzle-ring": "^2.0 || ^1.0",
        "phergie/phergie-irc-plugin-dns": "^4.0"
    },
    "require-dev": {
        "phergie/phergie-irc-bot-react-development": "^1.0.2"
    },
    "autoload": {
        "psr-4": {
            "Phergie\\Plugin\\Http\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Phergie\\Tests\\Plugin\\Http\\": "tests"
        }
    },
    "config": {
        "sort-packages": true,
        "platform": {
          "php": "7.6.19"
        }
    }
}
